# components
loadrt pp_dout names=LED,axes_enable
loadrt pp_instance 
loadrt threads name1=fast fp1=0 period1=50000 name2=slow period2=1000000 

# signals

# nets
net pi_instance pp_instance.0.pi-instance LED.pi-instance axes_enable.pi-instance
# parameter values
setp LED.bcm 25
setp LED.value 1

setp axes_enable.bcm 22
setp axes_enable.value 1


# realtime thread/function links
addf LED.write slow
addf axes_enable.write slow

